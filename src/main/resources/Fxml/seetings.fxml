                              </Label>
                              <PasswordField fx:id="newConfirmPassword" prefHeight="25.0" prefWidth="239.0">
                                 <font>
                                    <Font size="17.0" />
                                 </font>
                              </PasswordField>
                           </children>
                        </HBox>
                        <HBox fx:id="hbox5" layoutX="77.0" layoutY="233.0">
                           <children>
                              <Label layoutX="58.0" layoutY="35.0" prefWidth="200.0">
                                 <font>
                                    <Font name="System Bold" size="18.0" />
                                 </font>
                              </Label>
                              <JFXButton fx:id="savePassword" onAction="#savePasswordAction" prefHeight="39.0" prefWidth="165.0" style="-fx-background-color: #2ed573;" text="تعديل كلمه السر " textFill="WHITE">
                                 <font>
                                    <Font name="System Bold Italic" size="18.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </JFXButton>
                              <JFXButton fx:id="savePhone" onAction="#savePhoneAction" prefHeight="33.0" prefWidth="170.0" style="-fx-background-color: #2ed573;" text="تعديل الموبايل" textFill="WHITE">
                                 <font>
                                    <Font name="System Bold Italic" size="18.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </JFXButton>
                           </children>
                        </HBox>
                     </children>
                  </Pane>
               </content>
          </TitledPane>
          <TitledPane fx:id="pane3" text="عمل نسخة احتياطية">
