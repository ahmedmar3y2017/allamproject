                        <HBox fx:id="hbox1" layoutX="58.0" layoutY="35.0">
                           <children>
                              <Label layoutX="58.0" layoutY="35.0" prefWidth="170.0" text="رقم الموبايل  : ">
                                 <font>
                                    <Font name="System Bold" size="18.0" />
