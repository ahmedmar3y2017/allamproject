                         <graphic>
                            <ImageView fitHeight="70.0" fitWidth="50.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
                               <image>
                                  <Image url="@../images/forbidden.png" />
                               </image>
                            </ImageView>
                         </graphic>
                         <cursor>
                            <Cursor fx:constant="HAND" />
                         </cursor>
                      </JFXButton>

                   </children>
                </Pane>
             </content>

          </TitledPane>
        </panes>
